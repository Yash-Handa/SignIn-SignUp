{
  "name": "signin-signup",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "run-s lint-server lint-client",
    "start2": "node ./bin/www",
    "debug": "set DEBUG=SignIn-SignUp:* & npm run start2",
    "start": "run-p lint-w debug",
    "start-w": "nodemon --exec run-s prestart debug",
    "start-w-lite": "nodemon --exec npm run debug",
    "localTunnel": "lt --port 1998 --subdomain yash",
    "lt": "run-p start localTunnel",
    "lint-server": "esw --color",
    "lint-w": "esw --color --watch",
    "public": "cd public",
    "out": "cd..",
    "lint-client": "run-s public lint-server out"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "hbs": "~4.0.1",
    "helmet": "^3.16.0",
    "http-errors": "^1.7.1",
    "jsonwebtoken": "^8.5.0",
    "limiter": "^1.1.3",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.4.1",
    "morgan": "~1.9.0",
    "nodemailer": "^5.0.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "localtunnel": "^1.9.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "10.12.0",
    "npm": "6.5.0"
  }
}
